{
    "resources": [
        {
            "apiVersion": "2016-03-01",
            "type": "Microsoft.Web/sites",
            "name": "[variables('name')]",
            "properties": {
                "siteConfig": {
                    "appSettings": [
                        {
                            "name": "FUNCTIONS_WORKER_RUNTIME",
                            "value": "dotnet"
                        },
                        {
                            "name": "AzureWebJobsStorage",
                            "value": "[concat('DefaultEndpointsProtocol=https;AccountName=',variables('storagename'),';AccountKey=',listKeys(resourceId('Microsoft.Storage/storageAccounts', variables('storagename')), '2015-05-01-preview').key1)]"
                        },
                        {
                            "name": "FUNCTIONS_EXTENSION_VERSION",
                            "value": "~2"
                        },
                        {
                            "name": "WEBSITE_CONTENTAZUREFILECONNECTIONSTRING",
                            "value": "[concat('DefaultEndpointsProtocol=https;AccountName=',variables('storagename'),';AccountKey=',listKeys(resourceId('Microsoft.Storage/storageAccounts', variables('storagename')), '2015-05-01-preview').key1)]"
                        },
                        {
                            "name": "WEBSITE_CONTENTSHARE",
                            "value": "[concat(toLower(variables('name')), '8f5c')]"
                        },
                        {
                            "name": "WEBSITE_NODE_DEFAULT_VERSION",
                            "value": "8.11.1"
                        },
                        {
                            "name": "APPINSIGHTS_INSTRUMENTATIONKEY",
                            "value": "[reference(concat('microsoft.insights/components/', variables('insightname')) ,'2015-05-01').InstrumentationKey]"
                        }
                    ]
                },
                "clientAffinityEnabled": false,
                "reserved": false
            },
            "dependsOn": [
                "[resourceId('Microsoft.Storage/storageAccounts', variables('storagename'))]",
                "[concat('microsoft.insights/components/', variables('insightname'))]"
            ],
            "location": "[variables('location')]",
            "kind": "functionapp"
        },
        {
            "apiVersion": "2015-05-01-preview",
            "type": "Microsoft.Storage/storageAccounts",
            "name": "[variables('storagename')]",
            "location": "[variables('location')]",
            "properties": {
                "accountType": "Standard_LRS"
            }
        },
        {
            "apiVersion": "2015-05-01",
            "name": "[variables('insightname')]",
            "type": "microsoft.insights/components",
            "location": "[variables('location')]",
            "properties": {
                "ApplicationId": "[variables('insightname')]",
                "Request_Source": "IbizaWebAppExtensionCreate"
            }
        }
    ],
    "parameters": {
        "functionname": {
            "type": "string"
        },
        "insightname": {
            "type": "string"
        },
        "storagename": {
            "type": "string"
        }
    },
    "variables": {
        "storageName": "[toLower( concat( parameters('storagename'), uniqueString(resourceGroup().id) ) )]",
        "location": "West US 2",
        "name": "[toLower( concat( parameters('name'), uniqueString(resourceGroup().id) ) )]",
        "insightName": "[toLower( concat( parameters('insightname'), uniqueString(resourceGroup().id) ) )]"
      },
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0"
}